{
  "name": "DropPaper",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Drop Paper",
  "permissions": [
	"http://*/*",
	"https://*/*"
  ],
  "browser_action": {
    "default_icon": "dp_icon.png"
  },
  "background": {
    "page": "background.html"
  },
  "icons": {
    "48" : "dp_icon.png"
  },
  "content_security_policy": "script-src 'self' https://www.google.com/; object-src 'self'",
  "content_scripts": [
    {
		"matches": ["http://*/*","https://*/*"],
		"js" : ["contentscript.js"]
    }
  ]
}
